<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings 
	version="2.0" 
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	
	<jaxb:globalBindings>
       	<jaxb:javaType name="java.util.Date"  xmlType="xs:date" 
                    printMethod="com.psquickit.converter.xsd.CommonTypeConverter.printDate"   
                    parseMethod="com.psquickit.converter.xsd.CommonTypeConverter.parseDate"/>
        <jaxb:javaType name="java.util.Date"  xmlType="xs:dateTime" 
                    printMethod="com.psquickit.converter.xsd.CommonTypeConverter.printTimestamp"   
                    parseMethod="com.psquickit.converter.xsd.CommonTypeConverter.parseTimestamp"/>
        <jaxb:serializable />
	</jaxb:globalBindings>  
  
</jaxb:bindings>

